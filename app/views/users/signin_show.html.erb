<h1>Welcome to RIKKA BEAUTY</h1>
<h2>Log In</h2>

<%= form_for(:user, :url => {:controller => 'users', :action => 'create'}) do |f| %>
  <p>
    <%= f.label :Username %><br>
    <%= f.text_field :username %>
  </p>
  <p>
    <%= f.label :Password %><br>
    <%= f.text_field :password %>
  </p>
  <%= link_to '<button type="button">Log In</button>'.html_safe, "Users#show" %>
<% end %>



<%= link_to 'SignUp', signup_users_path %>

<%= link_to 'Forgot', forgot_users_path %>